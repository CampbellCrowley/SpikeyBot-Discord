{
  "name": "spikeybot-discord",
  "version": "1.10.3",
  "description": "SpikeyBot",
  "homepage": "https://www.spikeybot.com/",
  "bugs": {
    "email": "bugs@spikeybot.com"
  },
  "license": "UNLICENSED",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/CampbellCrowley/SpikeyBot-Discord"
  },
  "dependencies": {
    "@discordjs/uws": "^11.149.1",
    "@google-cloud/text-to-speech": "^0.5.1",
    "algebra.js": "^0.2.6",
    "async": "^2.6.2",
    "dateformat": "^3.0.3",
    "dialogflow": "^0.8.2",
    "discord.js": "github:discordjs/discord.js#89e27e507144dd3c32d644e0f298207eb538ff65",
    "erlpack": "github:discordapp/erlpack#674ebfd3439ba4b7ce616709821d27630f7cdc61",
    "fuzzy-search": "^3.0.1",
    "jimp": "^0.6.1",
    "mathjs": "^5.9.0",
    "mkdirp": "^0.5.1",
    "mysql": "^2.16.0",
    "node-opus": "^0.3.1",
    "ogg": "^1.2.6",
    "patreon": "^0.4.1",
    "rimraf": "^2.6.3",
    "socket.io": "^2.2.0",
    "sodium": "^3.0.2",
    "threads": "^0.12.1",
    "youtube-dl": "^1.13.1",
    "zlib-sync": "^0.1.4"
  },
  "contributors": [
    {
      "name": "Campbell Crowley",
      "email": "dev@campbellcrowley.com"
    }
  ],
  "devDependencies": {
    "acorn": "^6.1.1",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.12.0",
    "eslint-plugin-jsdoc": "^4.8.3",
    "jsdoc": "^3.5.5",
    "jsdoc-template": "github:braintree/jsdoc-template",
    "mocha": "^6.1.4"
  },
  "scripts": {
    "install": ".hooks/install-hooks.sh",
    "lint": "eslint --no-ignore $(find ./ -iname '*.js' -not -path './node_modules/*' -not -path './docs/*' -not -iname '.eslintrc.js' -printf '%p ')",
    "lint:fix": "eslint --fix --no-ignore $(find ./ -iname '*.js' -not -path './node_modules/*' -not -path './docs/*' -not -iname '.eslintrc.js' -printf '%p ')",
    "lintandtest": "npm run lint && npm run test",
    "lintforcommit": "TOLINT=\"$(git diff --cached --name-only --diff-filter=ACM | grep \".js$\" | grep -v eslintrc.js | grep -v \"^docs/\" | tr '\\n' ' ')\";if [ -z \"$TOLINT\" ]; then echo 'Nothing to lint'; else eslint $TOLINT; fi",
    "test": "mocha --delay",
    "test:hg": "mocha --delay --grep \"START|Hungry Games|END\"",
    "test:hg:npc": "mocha --delay --grep \"START|NPC|END\"",
    "test:main": "mocha --delay --grep \"START|Main Module|END\"",
    "run": "npm start",
    "run:rem": "npm run start:rem",
    "start": "node --experimental-worker src/SpikeyBot.js --dev --shards &",
    "start:rem": "node src/SpikeyBot.js --dev --shards --botname rembot &",
    "docs": "npm run lint && rm ./docs/*.html && bash .hooks/make-docs.sh",
    "update": "git checkout -- ./npm-shrinkwrap.json ./package.json && exec ssh-agent bash -c \"ssh-add ~/.ssh/sb_id_rsa_nopass && git pull && npm i && npm i youtube-dl\"; kill $SSH_AGENT_PID"
  }
}
