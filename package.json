    "eslint-config-google": "^0.9.1",
    "jsdoc-to-markdown": "^4.0.1",
    "mocha": "^5.1.1"
  },
  "scripts": {
    "install": ".hooks/install-hooks.sh",
    "lint": "eslint ./*.js ./**/*.js",
    "lint:fix": "eslint ./*.js ./**/*.js --fix",
    "lintandtest": "npm run lint && npm run test",
    "test": "mocha --delay",
    "test:hg": "mocha --delay --grep \"START|Hungry Games|END\"",
    "test:main": "mocha --delay --grep \"START|Main Module|END\"",
    "run": "npm run kill; (nodejs src/SpikeyBot.js dev onlymusic ./music.js &) && (nodejs src/SpikeyBot.js dev ./hungryGames.js ./main.js &)",
    "kill": "killall nodejs",
    "docs": "npm run lint && jsdoc2md src/*.js --private > docs/README.md"
  }
}
