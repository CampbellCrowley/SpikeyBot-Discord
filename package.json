{
  "name": "spikeybot-discord",
  "version": "1.7.4",
  "description": "SpikeyBot",
  "homepage": "https://www.spikeybot.com/",
  "bugs": {
    "email": "bugs@spikeybot.com"
  },
  "license": "UNLICENSED",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/CampbellCrowley/SpikeyBot-Discord"
  },
  "dependencies": {
    "@discordjs/uws": "^10.149.0",
    "@google-cloud/text-to-speech": "^0.3.0",
    "algebra.js": "^0.2.6",
    "async": "^2.6.1",
    "dateformat": "^3.0.3",
    "discord.js": "github:discordjs/discord.js",
    "fuzzy-search": "^2.1.0",
    "jimp": "^0.5.3",
    "mathjs": "^5.2.0",
    "mkdirp": "^0.5.1",
    "mysql": "^2.16.0",
    "node-opus": "^0.3.0",
    "ogg": "^1.2.5",
    "patreon": "^0.4.1",
    "rimraf": "^2.6.2",
    "socket.io": "^2.1.1",
    "threads": "^0.12.0",
    "youtube-dl": "^1.12.2"
  },
  "contributors": [
    {
      "name": "Campbell Crowley",
      "email": "dev@campbellcrowley.com"
    }
  ],
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.6.1",
    "eslint-config-google": "^0.10.0",
    "jsdoc-to-markdown": "^4.0.1",
    "mocha": "^5.2.0"
  },
  "scripts": {
    "install": ".hooks/install-hooks.sh",
    "lint": "eslint $(find ./ -iname '*.js' -not -path './node_modules/*' -not -iname '.eslintrc.js' -printf '%p ')",
    "lint:fix": "eslint --fix $(find ./ -iname '*.js' -not -path './node_modules/*' -not -iname '.eslintrc.js' -printf '%p ')",
    "lintandtest": "npm run lint && npm run test",
    "test": "mocha --delay",
    "test:hg": "mocha --delay --grep \"START|Hungry Games|END\"",
    "test:main": "mocha --delay --grep \"START|Main Module|END\"",
    "run": "npm run kill; npm start",
    "run:rem": "npm run kill; npm run start:rem",
    "start": "nodejs src/SpikeyBot.js --dev --shards ./web/proxy.js ./web/account.js ./web/stats.js ./hungryGames.js ./main.js ./ticTacToe.js ./connect4.js ./music.js ./patreon.js ./tts.js ./poll.js &",
    "start:rem": "nodejs src/SpikeyBot.js --dev --shards --botname rembot ./hungryGames.js ./main.js ./ticTacToe.js ./connect4.js ./music.js ./patreon.js ./tts.js ./poll.js &",
    "kill": "killall nodejs node; exit 0;",
    "docs": "npm run lint && bash .hooks/make-docs.sh",
    "docs:help": "nodejs src/lib/json2md.js docs/*.json",
    "update": "git checkout -- package-lock.json && exec ssh-agent bash -c \"ssh-add ~/.ssh/sb_id_rsa_nopass && git pull && npm i && npm i youtube-dl && npm up\"; kill $SSH_AGENT_PID"
  }
}
