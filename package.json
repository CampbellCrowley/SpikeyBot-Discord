  "dependencies": {
    "algebra.js": "^0.2.6",
    "async": "^2.6.0",
    "dateformat": "^3.0.3",
    "discord.js": "github:discordjs/discord.js",
    "jimp": "^0.2.28",
    "mathjs": "^4.1.1",
    "node-opus": "^0.2.7",
