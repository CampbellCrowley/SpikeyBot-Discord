{
  "name": "spikeybot-discord",
  "version": "1.5.14",
  "description": "SpikeyBot",
  "homepage": "https://www.spikeybot.com/",
  "bugs": {
    "email": "bugs@spikeybot.com"
  },
  "license": "UNLICENSED",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/CampbellCrowley/SpikeyBot-Discord"
  },
  "dependencies": {
    "algebra.js": "^0.2.6",
    "async": "^2.6.1",
    "dateformat": "^3.0.3",
    "discord.js": "github:discordjs/discord.js",
    "jimp": "^0.2.28",
    "mathjs": "^4.4.2",
    "node-opus": "^0.2.9",
    "ogg": "^1.2.5",
    "socket.io": "^2.1.1",
    "uws": "^9.148.0",
    "youtube-dl": "^1.12.2"
  },
  "contributors": [
    {
      "name": "Campbell Crowley",
      "email": "dev@campbellcrowley.com"
    }
  ],
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.9.1",
    "jsdoc-to-markdown": "^4.0.1",
    "mocha": "^5.2.0"
  },
  "scripts": {
    "install": ".hooks/install-hooks.sh",
    "lint": "eslint ./*.js ./**/*.js",
    "lint:fix": "eslint ./*.js ./**/*.js --fix",
    "lintandtest": "npm run lint && npm run test",
    "test": "mocha --delay",
    "test:hg": "mocha --delay --grep \"START|Hungry Games|END\"",
    "test:main": "mocha --delay --grep \"START|Main Module|END\"",
    "run": "npm run kill; npm start",
    "run:single": "npm run kill; npm run start:single",
    "start": "(nodejs src/SpikeyBot.js dev onlymusic ./music.js &) && (nodejs src/SpikeyBot.js dev ./hungryGames.js ./main.js ./ticTacToe.js ./connect4.js &)",
    "start:single": "nodejs src/SpikeyBot.js dev ./hungryGames.js ./main.js ./ticTacToe.js ./connect4.js ./music.js &",
    "kill": "killall nodejs",
    "docs": "npm run lint && bash .hooks/make-docs.sh",
    "docs:help": "nodejs src/json2md.js docs/*.json"
  }
}
